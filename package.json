{
  "name": "novetelo",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://MartinPucan.github.io/novetelo",
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.3",
    "@testing-library/user-event": "^12.6.2",
    "panicum": "^1.0.2",
    "parcel-bundler": "^1.12.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "web-vitals": "^0.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "dev:css": "parcel watch front/scss/*.scss --no-source-maps -d public/css/ --no-hmr --public-url ./",
    "build:css": "parcel build front/scss/*.scss --no-source-maps -d public/css/ --public-url ./",
    "dev:js": "parcel watch front/js/*.js --no-source-maps -d public/dist/js/",
    "build:js": "parcel build front/js/*.js --no-source-maps -d public/dist/js/ --public-url ./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.1.0",
    "sass": "^1.32.5"
  }
}
